				<content>
					<item>
						<label>$LOCALIZE[31909]</label>
						<icon />
						<visible>String.IsEmpty(Window.Property(Weekend.IsFetched))</visible>
					</item>
					<item>
						<label>$INFO[Window.Property(Weekend.1.LongDay)] $INFO[Window.Property(Weekend.1.LongDate)]</label>
